{
	"name": "fabianmichael/kirby-links",
	"type": "kirby-plugin",
	"version": "1.0.0-beta.3",
  "license": "MIT",
	"autoload": {
		"psr-4": {
			"FabianMichael\\Links\\": "src/"
		},
    "files": [
      "./helpers.php"
    ]
	},
	"authors": [
		{
			"name": "Fabian Michael",
			"email": "hallo@fabianmichael.de"
		}
	],
	"config": {
		"optimize-autoloader": true,
		"allow-plugins": {
			"getkirby/composer-installer": true
		}
	},
	"extra": {
		"installer-name": "links"
	},
  "require-dev": {
    "squizlabs/php_codesniffer": "^3.6"
  },
  "require": {
		"getkirby/composer-installer": "^1.2",
    "fabianmichael/kirby-template-attributes": "*"
  }
}
